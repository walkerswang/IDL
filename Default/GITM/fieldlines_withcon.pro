;n = 25722L;e = 19683L;filename = "B=25nT_isosurface.dat";fileout  = "bfield_iso.wrl";nVars = 13;n = 23134L;e = 25952Lfilename = "density=1.3_isosurface.dat"fileout  = "density_iso.wrl"nVars = 13n = 20796Le = 25629Lt = e*4+exyz = dblarr(nVars,n)ptemp = ulonarr(4,e)p = ulonarr(5,e)edges = ulonarr(t)openr,1,filenameline = ""while strpos(line,"DT") eq -1 do readf,1,linereadf,1,xyzreadf,1,ptempclose,1p(0,*) = 4p(1:4,*) = ptemp-1edges = popoly = obj_new('IDLgrPolygon',xyz(0:2,*),polygon=edges)omodel = obj_new('IDLgrModel')oview = obj_new('IDLgrView')omodel->Add,opolyoview->Add,omodelovrml = obj_new('IDLgrVRML',filename=fileout,quality=2)ovrml->Draw,oviewobj_destroy, opolyobj_destroy, omodelobj_destroy, oviewobj_destroy, ovrmlend